{
  "manifest_version": 3,
  "name": "Coupon loader",
  "version": "1.0",
  "version_name": "1.0 beta",
  "description": "Load digital coupons",
  "icons": {
    "16": "images/16x16.png",
    "32": "images/32x32.png",
    "48": "images/48x48.png",
    "128": "images/128x128.png"
  },
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_title": "Load Coupons",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background/background.js"
  }
}
